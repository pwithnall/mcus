<?xml version="1.0"?>
<interface>
	<object class="GtkUIManager" id="mcus_ui_manager">
		<child>
			<object class="GtkActionGroup" id="actiongroup1">
				<child>
					<object class="GtkAction" id="mcus_file_action">
						<property name="name">file</property>
						<property name="label">_File</property>
					</object>
				</child>
				<child>
					<object class="GtkAction" id="mcus_print_action">
						<property name="stock_id">gtk-print</property>
						<property name="name">file-print</property>
						<signal name="activate" handler="mw_print_activate_cb"/>
					</object>
				</child>
				<child>
					<object class="GtkAction" id="mcus_quit_action">
						<property name="stock_id">gtk-quit</property>
						<property name="name">file-quit</property>
						<signal name="activate" handler="mw_quit_activate_cb"/>
					</object>
				</child>
				<child>
					<object class="GtkAction" id="mcus_edit_action">
						<property name="name">edit</property>
						<property name="label">_Edit</property>
					</object>
				</child>
				<child>
					<object class="GtkAction" id="mcus_cut_action">
						<property name="stock_id">gtk-cut</property>
						<property name="name">edit-cut</property>
						<signal name="activate" handler="mw_cut_activate_cb"/>
					</object>
				</child>
				<child>
					<object class="GtkAction" id="mcus_copy_action">
						<property name="stock_id">gtk-copy</property>
						<property name="name">edit-copy</property>
						<signal name="activate" handler="mw_copy_activate_cb"/>
					</object>
				</child>
				<child>
					<object class="GtkAction" id="mcus_paste_action">
						<property name="stock_id">gtk-paste</property>
						<property name="name">edit-paste</property>
						<signal name="activate" handler="mw_paste_activate_cb"/>
					</object>
				</child>
				<child>
					<object class="GtkAction" id="mcus_delete_action">
						<property name="stock_id">gtk-delete</property>
						<property name="name">edit-delete</property>
						<signal name="activate" handler="mw_delete_activate_cb"/>
					</object>
				</child>
				<child>
					<object class="GtkAction" id="mcus_help_action">
						<property name="name">help</property>
						<property name="label">_Help</property>
					</object>
				</child>
				<child>
					<object class="GtkAction" id="mcus_about_action">
						<property name="stock_id">gtk-about</property>
						<property name="name">help-about</property>
						<signal name="activate" handler="mw_about_activate_cb"/>
					</object>
				</child>
			</object>
		</child>
		<ui>
			<menubar name="mw_menu_bar">
				<menu action="mcus_file_action">
					<menuitem action="mcus_print_action"/>
					<menuitem action="mcus_quit_action"/>
				</menu>
				<menu action="mcus_edit_action">
					<menuitem action="mcus_cut_action"/>
					<menuitem action="mcus_copy_action"/>
					<menuitem action="mcus_paste_action"/>
					<menuitem action="mcus_delete_action"/>
				</menu>
				<menu action="mcus_help_action">
					<menuitem action="mcus_about_action"/>
				</menu>
			</menubar>
		</ui>
	</object>

	<object class="GtkTextBuffer" id="mw_code_buffer"/>

	<object class="GtkWindow" id="mcus_main_window">
		<property name="title" translatable="yes">Microcontroller Simulator</property>
		<signal name="delete-event" handler="mw_delete_event_cb"/>
		<child>
			<object class="GtkVBox" id="vbox1">
				<child>
					<object class="GtkMenuBar" constructor="mcus_ui_manager" id="mw_menu_bar"/>
					<packing>
						<property name="expand">False</property>
					</packing>
				</child>
				<child>
					<object class="GtkHBox" id="hbox1">
						<child>
							<object class="GtkScrolledWindow" id="scrolledwindow1">
								<property name="hscrollbar-policy">GTK_POLICY_AUTOMATIC</property>
								<property name="vscrollbar-policy">GTK_POLICY_AUTOMATIC</property>
								<property name="shadow-type">GTK_SHADOW_IN</property>
								<child>
									<object class="GtkTextView" id="mw_code_view">
										<property name="buffer">mw_code_buffer</property>
									</object>
								</child>
							</object>
						</child>
						<child>
							<object class="GtkVBox" id="vbox3">
								<child>
									<object class="GtkLabel" id="label1">
										<property name="label" translatable="yes">&lt;b&gt;Inputs/Outputs&lt;/b&gt;</property>
										<property name="use-markup">True</property>
									</object>
									<packing>
										<property name="expand">False</property>
									</packing>
								</child>
								<child>
									<object class="GtkEntry" id="mw_input_port_entry"/>
								</child>
								<child>
									<object class="GtkEntry" id="mw_output_port_entry"/>
								</child>
								<child>
									<object class="GtkEntry" id="mw_adc_entry"/>
								</child>
								<child>
									<object class="GtkLabel" id="label2">
										<property name="label" translatable="yes">&lt;b&gt;Registers&lt;/b&gt;</property>
										<property name="use-markup">True</property>
									</object>
									<packing>
										<property name="expand">False</property>
									</packing>
								</child>
								<child>
									<object class="GtkEntry" id="mw_register0_entry"/>
								</child>
								<child>
									<object class="GtkEntry" id="mw_register1_entry"/>
								</child>
								<child>
									<object class="GtkEntry" id="mw_register2_entry"/>
								</child>
								<child>
									<object class="GtkEntry" id="mw_register3_entry"/>
								</child>
								<child>
									<object class="GtkEntry" id="mw_register4_entry"/>
								</child>
								<child>
									<object class="GtkEntry" id="mw_register5_entry"/>
								</child>
								<child>
									<object class="GtkEntry" id="mw_register6_entry"/>
								</child>
								<child>
									<object class="GtkEntry" id="mw_register7_entry"/>
								</child>
								<child>
									<object class="GtkLabel" id="label2">
										<property name="label" translatable="yes">&lt;b&gt;Memory&lt;/b&gt;</property>
										<property name="use-markup">True</property>
									</object>
									<packing>
										<property name="expand">False</property>
									</packing>
								</child>
								<child>
									<object class="GtkEntry" id="mw_memory_entry"/>
								</child>
							</object>
							<packing>
								<property name="expand">False</property>
							</packing>
						</child>
					</object>
				</child>
				<child>
					<object class="GtkStatusbar" id="mw_statusbar">
						<property name="spacing">2</property>
					</object>
					<packing>
						<property name="expand">False</property>
					</packing>
				</child>
			</object>
		</child>
	</object>
</interface>
